{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": true
    },
    "target": "es2020",
    "externalHelpers": false,
    "keepClassNames": true,
    "loose": true
  },
  "module": {
    "type": "es6"
  },
  "minify": false,
  "isModule": true,
  "env": {
    "targets": {
      "browsers": [
        "last 2 versions",
        "not dead",
        "not IE 11"
      ]
    }
  }
} 